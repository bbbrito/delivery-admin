{
  "name": "delivery-admin",
  "private": true,
  "version": "0.0.1",
  "description": "",
  "main": "server/app.js",
  "bin": "server/bin/www",
  "scripts": {
    "start": "npm-run-all --parallel server gulp gulp-watch karma",
    "gulp": "gulp",
    "gulp-watch": "gulp watch",
    "karma": "./node_modules/karma/bin/karma start --browsers PhantomJS ./tests/angular/karma.conf.js",
    "server": "export DEBUG=delivery-admin:*; nodemon ./server/bin/www",
    "test": "export NODE_ENV=test; export DEBUG=delivery-admin:*; ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha tests/nodejs/* --exit"
  },
  "os": [
    "darwin",
    "linux",
    "!win32"
  ],
  "engines": {
    "node": "~10.0.0",
    "npm": "~6.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wbruno/delivery-admin.git"
  },
  "bugs": {
    "url": "https://github.com/wbruno/delivery-admin/issues"
  },
  "homepage": "https://github.com/wbruno/delivery-admin#readme",
  "author": "William B. <wbrunom@gmail.com> (http://wbruno.com.br)",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "config": "^1.21.0",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "mongojs": "^2.6.0",
    "mongoose": "^5.7.1"
  },
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-angular-templatecache": "^3.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-htmlmin": "^5.0.1",
    "gulp-livereload": "^4.0.2",
    "gulp-minify-css": "^1.2.4",
    "gulp-ng-annotate": "^2.1.0",
    "gulp-stylus": "^2.7.0",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.8",
    "istanbul": "^0.4.5",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "npm-run-all": "^3.1.0",
    "supertest": "^3.4.2",
    "wiredep": "^4.0.0"
  },
  "wiredep": {
    "ignorePath": "public/",
    "exclude": "angular-mocks",
    "overrides": {
      "font-awesome": {
        "main": [
          "./css/font-awesome.css",
          "./fonts/*"
        ]
      },
      "bootstrap": {
        "main": [
          "dist/css/bootstrap.min.css",
          "dist/js/bootstrap.min.js"
        ]
      }
    }
  }
}
